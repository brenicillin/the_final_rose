<h1 id="contestant-name"><%= @contestant.name %></h1>

<body>
  <section id="contestant-season">
    <h2>Season: <%= @contestant.find_season %> - <%= @contestant.season_description %></h2>
  </section>

  <section id="contestant-outings">
    <h2>Outings</h2>
    <ul>
      <% @contestant.outings.each do |outing| %>
        <li><%= link_to "#{outing.name}", outing_path(outing) %></li>
      <% end %>
    </ul>
  </section>
</body>